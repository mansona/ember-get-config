{
  "name": "ember-get-config",
  "version": "0.3.0",
  "description": "Get `config/environment` from anywhere, even addons!!!",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/patience-tema-baron/ember-get-config",
  "scripts": {
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "ember-compatibility-test": "ember try:each"
  },
  "dependencies": {
    "broccoli-file-creator": "^1.1.1",
    "ember-cli-babel": "^7.7.3"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "auto-changelog": "https://git@github.com/mansona/auto-changelog.git#epic",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.12.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.12.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "ember-welcome-page": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^9.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.4"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
